<script>
let topology = gon.topologies

let switches = []
let hosts = []
let selections = []

// スイッチとホストを分けて取り出す
for (var i=0; i<topology.length; i++) {
  if (topology[i].dport2 != null) {
    if (topology[i].dport2.match(/-/)) {
      switches.push(topology[i].dport1.split("-")[0], topology[i].dport2.split("-")[0]);
    }else {
      hosts.push(topology[i].dport2);
      selections.push(topology[i].dport1);
    }
  }
}
</script>
